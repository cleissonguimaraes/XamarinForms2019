        <ToolbarItem Text="Pesquisa" />
        <ToolbarItem Text="Detalhes" />
        <ToolbarItem Text="Ordenar" />
    </ContentPage.ToolbarItems>

    <ContentPage.Content>
        <StackLayout>
            <Label Text="Nenhum Relatório"
